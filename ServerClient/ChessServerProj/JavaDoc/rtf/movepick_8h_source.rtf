37 {\cf17 class }StatsEntry \{\par
38 \par
39   T entry;\par
40 \par
41 {\cf17 public}:\par
42   {\cf18 void} operator=({\cf17 const} T& v) \{ entry = v; \}\par
43   T* operator&() \{ {\cf19 return} &entry; \}\par
44   T* operator->() \{ {\cf19 return} &entry; \}\par
45   {\cf17 operator} {\cf17 const} T&() {\cf17 const} \{ {\cf19 return} entry; \}\par
46 \par
47   {\cf18 void} operator<<({\cf18 int} bonus) \{\par
48     assert(abs(bonus) <= D); {\cf20 // Ensure range is [-D, D]}\par
49     {\cf17 static_assert}(D <= std::numeric_limits<T>::max(), {\cf22 "D overflows T"});\par
50 \par
51     entry += bonus - entry * abs(bonus) / D;\par
52 \par
53     assert(abs(entry) <= D);\par
54   \}\par
55 \};\par
