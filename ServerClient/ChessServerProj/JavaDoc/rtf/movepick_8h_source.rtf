70     assert(std::is_standard_layout<stats>::value);\par
71 \par
72     {\cf17 typedef} StatsEntry<T, D> entry;\par
73     entry* p = {\cf17 reinterpret_cast<}entry*{\cf17 >}({\cf17 this});\par
74     std::fill(p, p + {\cf17 sizeof}(*{\cf17 this}) / {\cf17 sizeof}(entry), v);\par
